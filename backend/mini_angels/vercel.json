{
    "builds": [
        {
            "src": "mini_angels/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "maxLamdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mini_angels/wsgi.py"
        }
    ]
}